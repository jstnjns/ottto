{
  "presets": [
    "react-native"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./app"],
      "alias": {
        "actions": "./app/actions",
        "components": "./app/components",
        "containers": "./app/containers"
      },
      "extensions": [".js", ".ios.js", ".android.js"]
    }]
  ]
}
